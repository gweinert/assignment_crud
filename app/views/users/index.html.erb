<h1>Users</h1>
<ul class="list-group">
  <% @users.each do |user| %>
    <li class="list-group">
      <%= link_to user.name, user_path(user) %>
      <%= link_to "edit", edit_user_path(user) %>
      <%= link_to "delete", user_path(user), method: :delete, data: {confirm: "Really delete the article?"}  %>

    </li>
  <% end %>
</ul>
<%= link_to "Create a User", new_user_path %>
